@model MVC_First.Models.quizResult

@{
    ViewBag.Title = "GenerateResult";
}

<div class ="container" style ="background-color:mintcream; height:100px; width:900px"> 
<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th style ="font-size:medium">
            Quiz ID
        </th>
        <th style ="font-size:medium">
            Student ID
        </th>
        <th style ="font-size:medium">
            Course ID
        </th>
        <th style ="font-size:medium">
            Teacher ID
        </th>
        <th style ="font-size:medium">
            Attempted
        </th>
        <th style ="font-size:medium">
            Correct
        </th>
        <th style ="font-size:medium">
            Wrong
        </th>
        <th style ="font-size:medium">
            Blank
        </th>
        <th style ="font-size:medium">
            Total Marks
        </th>
        <th style ="font-size:medium">
            @Html.DisplayNameFor(model => model.obtainMarks)
        </th>
        <th></th>
    </tr>
    <tr>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.qid)
        </td>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.sid)
        </td>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.cid)
        </td>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.tid)
        </td>
        <td style ="font-size:small">
            @ViewBag.attempted
        </td>
        <td style ="font-size:small">
            @ViewBag.correct
        </td>
        <td style ="font-size:small">
            @ViewBag.wrong
        </td>
        <td style ="font-size:small">
            @ViewBag.blank
        </td>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.totalmarks)
        </td>
        <td style ="font-size:small">
            @Html.DisplayFor(model => model.obtainMarks)
        </td>
        
    </tr>

</table>
    <br><br><br><div style ="width:200px;position:relative;top:-30px; margin-left:800px; font-size:xx-large;">
             @Html.ActionLink("Back", "StudentHome")
     </div>   
    </div>